            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python SAC",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/epic_mc.py",
            "args": [
                "--run", "1",
                "--env", "LunarLander-v2-cont",
                "--device", "cuda",
                "--learner", "sac",
                "--meta_update_every", "10",
                "--steps", "300",
                "--mass", "5",
                "--m", "2",
                "--goal", "10.0",
                "--resdir", "results/montecarlo/testsac",
                "--replay-capacity", "2"               
        ],
        "env": {
            "WANDB_MODE": "disabled",
            "CC": "gcc-12"
        }
        }
    ]
}